{
    "build": {
        "options": {
            "presets": [ "es2015" ],
            "plugins": [ "transform-es2015-modules-amd" ],
            "ignore": [ "src/js/bootstrap.js" ]
        },
        "files": [ {
            "expand": true,
            "cwd": "src/js/",
            "src": [ "**/*.js" ],
            "dest": "build/convert/"
        } ]
    }
}
